@model JVLiga.Models.Admin.SeasonModel

@{
    ViewData["Title"] = "Vytvoření sezóny";
}

<h2>Vytvoření sezóny</h2>

<div class="row">
	<div class="col-md-4">
		<form method="post">
			<hr />
			<div class="form-group">
				<label asp-for="Year"></label>
				<input asp-for="Year" class="form-control" />
				<span asp-validation-for="Year" class="text-danger"></span>
			</div>
			<button type="submit" class="btn btn-default">Vygenerovat</button>
		</form>
	</div>
</div>
